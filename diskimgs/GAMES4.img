	CALL	MFETCH		;select 1st message - WHAT!
	JP	ENTRI		;and try again
GOTALOT: LD	HL,CODBUF	;Save line in spare buffer.
	LD	DE,CODB2
	LD	BC,15
	LDIR
	RET			;else return.
;
FIND1ST: LD	A,(DE)		;Get 1st 4 letters and pad
	OR	A		;if necessary
	RET	Z
	CP	20H
	JR	NZ,BRP
	I
BOX:	POP	AF
