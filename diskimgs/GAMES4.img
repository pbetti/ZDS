CCOUNT:	DEFB	00	;Character count
SPEC:	DEFB	00	;Status of SPECIAL text
	DEFS	16	;STACK for 'SPECIAL' message system
NSTORE:	DEFS	4	;Temporary store for BIN-ASCII
SAVIX:	DEFB	00	;Temporary store
