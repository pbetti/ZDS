	SHLD	TADDR
	CALL	RDE
	CNZ	ERROR
	LDA	RQST
	CPI	2
	JZ	DIAG3
	CPI	3
	JZ	RINCR
	CPI	4
	JZ	DINCR
	CALL	COMPR	; IF DIAG 1, COMPARE
DIAG3	LXI	H,SCTR
	INR	M	; INC SECTOR
	LDA	NSCTRS
	INR	A
	CMP	M
	JNZ	DIAG1	; IF NOT END 	B
	JNC	RINCR1	;IF NO>NSCTRS TRY AGAIN
ONCM	LDA	NTRKS
	MOV	B,A
	CALL	PRAND
	ANI	07FH
	STA	TRK
	CMP	B
	JNC	ONCM	; IF NO>NTRKS TRY AGAIN
	JMP	DIAG1
;
;
;
; PSEUDO RANDOM NUMBER GENERATOR
