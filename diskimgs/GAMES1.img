;
PRAND	PUSH	D
	PUSH	B
	LXI	H,NUMB	; LOCATION OF SEED
	MVI	C,0
	MOV	A,M
	ANA	A
	JNZ	NEXT
	MVI	A,0FFH
NEXT	MOV	D,A
	ANI	1DH
	JPE	PAR
	MVI	C,80H
PAR	MOV	A,D
	RRC
	ANI	7FH
	ADD	C
	MOV	M,A
	POP	B
	POP	D
	RET
