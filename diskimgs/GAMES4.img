;STORAGE AREAS AND BUFFERS
;
XNCO:	DEFB	00	;1st Optional Message
XBEFA:	DEFB	00	;2nd Optional Message
LSTOBJ:	DEFB	00	;Byte reserved for latest object name
BUFFER:	DEFS	60	;60 byte line buffer
