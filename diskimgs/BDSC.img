prnt(str)	/* print given text, automatically filling to length of line */
char *str;
{
	char c;

	while (c = *str++)
	{
		if (c != '\n' && (wdlen(str) + column++) < (MAXCOL - 3))
		{
			putchar(c);
			if (!*str)
				putchar(' ');
			continue;
		}

		putchar('\n');

		if (!isspace(c))
			putchar(c);

		column = 1;
	}
}


